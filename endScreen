def setup():
    size(752,480)

def draw():
    score1 = 100000
    score2 = 121020
    background(51,102,51)
    textSize(100)
    fill(204,0,0)
    text("STAGE", 30, 100)
    fill(153,255,0)
    text("CLEAR", 400, 100)
    
    textSize(24)
   
    fill(255,255,255)
    text("Score : " + str(score1), 340,200) 
    fill(255,255,255)
    text("Coins : " + str(score2), 340,240) 
    fill(255,255,255)
    text("Treasure Complete", 250,290) 
    
